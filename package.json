{
  "name": "finscrape",
  "version": "0.0.1",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "mocha": "mocha 'test/**/*.test.ts' --check-leaks --fail-zero --forbid-only --forbid-pending",
