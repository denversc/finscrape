  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tsconfig/node24": "^24.0.0",
    "@types/chai": "^5.2.2",
    "@types/electron-prompt": "^1.6.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.18",
    "@types/signale": "^1.4.7",
    "@types/yargs": "^17.0.33",
    "chai": "^5.2.0",
    "eslint": "^9.23.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
